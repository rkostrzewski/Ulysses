<UserControl x:Class="Ulysses.App.Modules.ControlPanel.Views.ControlPanelView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:content="clr-namespace:Ulysses.App.Modules.Content"
             mc:Ignorable="d"
             d:DesignHeight="300" d:DesignWidth="150">
    <Grid Background="#FF8FB4BF">
        <StackPanel>
            <Button Command="{Binding ChangeContentRegionCommand }" 
                    CommandParameter="{ x:Static content:ContentViews.ImageDisplay  }"
                    Margin="0,10,0,0" Height="30"
                    VerticalAlignment="Top" Content="Image Display" />
            <Button Command="{Binding ChangeContentRegionCommand }" 
                    CommandParameter="{ x:Static content:ContentViews.ImageProcessingCustomization  }"
                    Margin="0,10,0,0" Height="30"
                    VerticalAlignment="Top" Content="Processing Customization" />
        </StackPanel>
    </Grid>
</UserControl>